<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.webwalker.appdemo">

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.HardwareSpeedupActivity"
            android:label="@string/title_activity_hardware_speedup" />
        <activity
            android:name=".activity.NormalActivity"
            android:label="@string/title_activity_normal" />
        <activity
            android:name="com.webwalker.framework.system.log.DebugActivity"
            android:label="@string/title_activity_debug" />
        <activity android:name=".web.X5Activity" />

        <service
            android:name=".web.FirstLoadingX5Service"
            android:process=":ymtservice" />

        <activity
            android:name=".web.WebViewActivity"
            android:label="@string/title_activity_web_view"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activity.JniActivity" />
        <activity android:name=".activity.AesActivity" />
        <activity android:name=".activity.AesRsaActivity" />
        <activity android:name=".activity.TempActivity" />
        <activity android:name=".activity.RecycleViewActivity"></activity>
    </application>

</manifest>