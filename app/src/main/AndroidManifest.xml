<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.webwalker.appdemo">

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.webwalker.framework.system.log.DebugActivity"
            android:label="@string/title_activity_debug" />
        <activity android:name=".web.X5Activity" />

        <service
            android:name=".web.FirstLoadingX5Service"
            android:process=":ymtservice" />

        <activity
            android:name=".web.WebViewActivity"
            android:label="@string/title_activity_web_view"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activity.JniActivity" />
        <activity android:name=".activity.AesActivity" />
        <activity android:name=".activity.AesRsaActivity" />
        <activity android:name=".activity.TestActivity" />
        <activity android:name=".activity.RecycleViewActivity" />
        <activity android:name=".activity.CoordinatorActivity" />
        <activity android:name=".activity.WebTestActivity" />
        <activity android:name=".activity.PatchActivity" />
        <activity android:name=".activity.TextViewActivity" />
        <activity android:name=".activity.DragActivity" />
        <activity android:name=".activity.YoutubeActivity" />
        <activity android:name=".activity.SlideDragActivity" />
        <activity android:name=".activity.UIActivity" />
        <activity android:name=".activity.SnackbarActivity" />
        <activity android:name=".activity.AnimationActivity" />
        <activity android:name=".activity.FrescoActivity" />
        <activity android:name=".activity.DrawActivity" />
        <activity android:name=".activity.SVGActivity" />
        <activity android:name=".activity.ViewPagerBannerActivity"></activity>
    </application>

</manifest>